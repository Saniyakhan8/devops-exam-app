pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
               git branch: 'main', url: 'https://github.com/Saniyakhan8/devops-exam-app.git'
            }
        }
stage('Build Docker Image') {
    steps {
        sh 'docker build -t devops-exam-app .'
    }
}


       stage('Run Container') {
    steps {
        sh 'docker run -d -p 9090:9090 devops-exam-app'
    }
}

    }
}
